{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.cjs": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "index.cjs"
    }
  ]
}
