{
  "version": 2,
  "buildCommand": "npx tsx script/build.ts",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
